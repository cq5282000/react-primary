
.content-padded{
	padding: 15px;
	.input-style{
		width: 200px;
	}
	.checkbox-wrapper{
		margin-top: 15px;
	}
}
.img-wrapper{
	width: 200px;
	height: 200px;
	background: url('./1.png');
	img{
		width: 100px;
		height: 100px;
		display: inline-block;
	}
}
.finding-wrapper{
	position: relative;
	display: block;
	width: 1000px;
	height: 1000px;
	color: #ffffff;
	.finding-bg{
		width:700px;
		height:700px;
		border-radius:100%;
		background-color:#29282d;
		position: absolute;
		margin:auto;
		top:0;
		left:0;
		bottom:70px;
		right:0;
		opacity:0;
		transition:all 1s;
		opacity: 1;
		display: inline-block;
		color: #ffffff;
	}
	.finding-bg-inner{
		width:700px;
		height:700px;
		// background-image:url('./1.png');
		background-image: url("data:image/png;base64,");
		background-repeat:repeat-x;
		background-size:1421px 700px;
		opacity: 0.07;
		color: #ffffff;
		animation:k-finding-bg-idle 45s infinite linear;
	}
	.finding-circle{
		perspective:1200px;
		perspective-origin:center center;
		position:absolute;
		margin:auto;
		top:0;
		left:0;
		bottom:70px;
		right:0;
		width:500px;
		height:500px;
		.c-index-0{
			top:25%;
			left:25%;
			width:50%;
			height:50%;
			border-radius:100%;
			border:1px solid #fff;
			position:absolute;
			animation:idle-rotate-0 4s linear infinite;
		}
		.c-index-1{
			top:15%;
			left:15%;
			width:70%;
			height:70%;
			border-radius:100%;
			border:2px solid rgba(255, 255, 255, 0.7);
			position:absolute;
			animation:idle-rotate-1 4s linear infinite;
		}
		.c-index-2{
			width:100%;
			height:100%;
			border-radius:100%;
			border:3px solid rgba(255, 255, 255, 0.4);
			position:absolute;
			animation:idle-rotate-2 4s linear infinite;
		}
		
	}
}
@keyframes idle-rotate-0 {
 from {
    transform: rotate3d(-0.2,1,-0.4, 0);
 }
 
 to {
    transform: rotate3d(-0.2,1,-0.4, 360deg);
 }
}


@keyframes idle-rotate-1 {
 from {
    transform: rotate3d(1,0.5,-0.1, 180deg);
 }
 
 to {
    transform: rotate3d(1,0.5,-0.1, 540deg);
 }
}

@keyframes idle-rotate-2 {
 from {
    transform: rotate3d(-0.2,1,-0.1, 360deg);
 }
 
 to {
    transform: rotate3d(-0.2,1,-0.1, 0);
 }
}
@keyframes k-finding-bg-idle {
  0% {
    background-position: -600px 0;
  }

  100% {
    background-position: -2021px 0;
  }
}

@keyframes idle-rotate-0-ios {
  0% {opacity: 0;}
  44% {opacity: 0.4;transform: scale(1.08,1.08);}
  90% {opacity: 0;transform: scale(1.15,1.15);}
  100% {opacity: 0;transform: scale(1.15,1.15);}
}

@keyframes idle-rotate-1-ios {
  0% {opacity: 0;}
  42% {opacity: 0.4;transform: scale(1.08,1.08);}
  90% {opacity: 0;transform: scale(1.15,1.15);}
  100% {opacity: 0;transform: scale(1.15,1.15);}
}

@keyframes idle-rotate-2-ios {
  0% {opacity: 0;}
  40% {opacity: 0.4;transform: scale(1.08,1.08);}
  90% {opacity: 0;transform: scale(1.15,1.15);}
  100% {opacity: 0;transform: scale(1.15,1.15);}
}

.pk-wrapper {
	margin-top: 1000px;
	position: relative;
	display: block;
	width: 500px;
	height: 700px;
	color: #ffffff;
	background: #000000;
	.pk-board-cnter-b{
		position:absolute;
		bottom:0;
		right:0;
		box-sizing: border-box;
		animation:pk-bottom-fade-in 1.5s;
		.pk-board-cnter-b-bg{
			width:900px;
			height:900px;
			background-image:linear-gradient( 135deg, rgb(66, 236, 203) 0%, rgb(74, 139 ,239) 50%);
			transform:rotate(35deg) translate(700px, 400px);
			transform-origin:center;
			box-sizing: border-box;
		}
	}
	.pk-board-cnter-t {
		position: absolute;
		top: 0;
		left: 0;
		box-sizing: border-box;
		animation:pk-top-fade-in 1.5s;
		.pk-board-cnter-t-bg{
			width: 900px;
			height: 900px;
			background-image: linear-gradient( 135deg, rgb(250, 163, 81) 60%, rgb(250, 121, 74) 100%);
			transform: rotate(35deg) translate(-700px, -400px);
			transform-origin: center;
		}
		
	}
	.pk-flash-mask {
		width: 100%;
		height: 100vh;
		background-color: #fff;
		opacity: 0;
		animation: pk-flash 1.5s;
	}
	.pk-indicate{
		width: 400px;
		height: 237px;
		background: url('./pk.png');
		background-size: cover;
		font-size: 120px;
		color: rgb(255,102,52);
		text-align: center;
		position: absolute;
		font-weight: bold;

		top: 0;
		left: 0;
		right: 0;
		bottom: 70px;
		margin: auto;
		transform-origin: 50% 70%;
		animation: pk-indicate-stamp 2.0s cubic-bezier(0.3,1.22,0.64,1);
		opacity: 1;  
	}
	.pk-indicate-1{
		width: 400px;
		height: 237px;
		background: url('./pk.png');
		background-size: cover;
		font-size: 120px;
		color: rgb(255,102,52);
		text-align: center;
		position: absolute;
		font-weight: bold;

		top: 0;
		left: 0;
		right: 0;
		bottom: 70px;
		margin: auto;
		transform-origin: 50% 70%;
		animation: pk-indicate-stamp-1 2.5s linear;
    	opacity: 0; 
	}
}

@keyframes pk-bottom-fade-in {
  0% {transform: translate(0, 600px);}
  30% {transform: translate(-95px, -17vh);}
  56% {transform: translate(40px, -9vh);}
  76% {transform: translate(-105px, -14vh);}
  100% {transform: translate(0, 0);}
}

@keyframes pk-top-fade-in {
  0% { transform: translate(0, -600px);}
  30% {transform: translate(95px, 17vh);}
  56% { transform: translate(-40px, 9vh);}
  76% { transform: translate(105px, 14vh);}
  100% { transform: translate(0, 0);}
}

@keyframes pk-flash {
	0% {opacity: 0;}

	25% {opacity: 0;}
	30% {opacity: 1;}
	35% {opacity: 0;}

	72% {opacity: 0;}
	76% {opacity: 1;}
	80% {opacity: 0;}

	100% {
		opacity: 0;
	}
}

@keyframes pk-indicate-stamp {

	0% {
		opacity: 0;
		transform: scale(8, 8);
	}

	45% {
		opacity: 0;
		transform: scale(8, 8);
	}

	65% {
		opacity: 1;
		transform: scale(1, 1);
	}

	100% {
		opacity: 1;
		transform: scale(1, 1);
	}

}


@keyframes pk-indicate-stamp-1 {

	0% {
		opacity: 0;
		transform: scale(1, 1);
	}

	48% {
		opacity: 0;
		transform: scale(1, 1);
	}

	56% {
		
		opacity: 0.4;
		transform: scale(1.05, 1.05);
	}

	72% {
		
		opacity: 0;
		transform: scale(1.15, 1.15);
	}

	100% {
		
		opacity: 0;
		transform: scale(1.15, 1.15);
	}

}
